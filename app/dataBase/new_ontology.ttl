<http://schema.org/name> a owl:DatatypeProperty ;
    rdfs:domain pdx:Pokemon ;
    rdfs:range xsd:string .

pdx:Battle a owl:Class .

pdx:evolvesTo a owl:ObjectProperty ;
    rdfs:domain   pdx:Pokemon ;
    rdfs:range    pdx:Pokemon ;
    owl:inverseOf pdx:evolvesFrom .

pdx:evolvesFrom a owl:ObjectProperty ;
    rdfs:domain   pdx:Pokemon ;
    rdfs:range    pdx:Pokemon ;
    owl:inverseOf pdx:evolvesTo .

pdx:LegendaryPokemon
    a owl:Class ;
    rdfs:subClassOf pdx:Pokemon ;
    owl:equivalentClass [
      a owl:Restriction ;
      owl:onProperty pdx:isLegendary ;
      owl:hasValue "true"^^xsd:boolean
    ] .

pdx:DualTypePokemon
    a owl:Class ;
    rdfs:subClassOf pdx:Pokemon ;
    owl:equivalentClass [
      a owl:Restriction ;
      owl:onProperty pdx:secondaryType ;
      owl:someValuesFrom pdx:Type
    ] .

pdx:MegaEvolution
    a owl:Class ;
    rdfs:subClassOf pdx:Pokemon ;
    owl:equivalentClass [
      a owl:Restriction ;
      owl:onProperty pdx:megaEvolutionOf ;
      owl:someValuesFrom pdx:Pokemon
    ] .

pdx:FighterPokemon
    a owl:Class ;
    rdfs:subClassOf pdx:Pokemon ;
    owl:equivalentClass [
      a owl:Restriction ;
      owl:onProperty pdx:participatesIn ;
      owl:someValuesFrom pdx:Battle
    ] .

pdx:GlassCannon
    a owl:Class ;
    rdfs:subClassOf pdx:Pokemon ;
    owl:equivalentClass [
      a owl:Class ;
      owl:intersectionOf (
        pdx:Pokemon
        [ a owl:Restriction ;
          owl:onProperty pdx:attack ;
          owl:someValuesFrom [
            a rdfs:Datatype ;
            owl:onDatatype xsd:integer ;
            owl:withRestrictions ( [ xsd:minExclusive "100"^^xsd:integer ] )
          ]
        ]
        [ a owl:Restriction ;
          owl:onProperty pdx:defense ;
          owl:someValuesFrom [
            a rdfs:Datatype ;
            owl:onDatatype xsd:integer ;
            owl:withRestrictions ( [ xsd:maxExclusive "50"^^xsd:integer ] )
          ]
        ]
      )
    ] .

pdx:secondaryType a owl:ObjectProperty ;
    rdfs:domain pdx:Pokemon ;
    rdfs:range pdx:Type .
    
:pdx:bestMatchAgainst a owl:ObjectProperty ;
    rdfs:domain    pdx:Pokemon ;
    rdfs:range     pdx:Pokemon ;
    rdfs:label     "Best Match Against" ;
    rdfs:comment   "Links a Pok√©mon to another that is a highly effective counter." .

pdx:Ability a owl:Class .

pdx:Type a owl:Class .

pdx:Effectiveness a owl:Class .

pdx:Pokemon a owl:Class ;
    owl:disjointWith pdx:Type .

